{
  "name": "express-ts-webpack-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.12.2",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/webpack-node-externals": "^3.0.4",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "^3.5.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "start": "node dist/index.cjs",
    "dev": "webpack watch --mode development",
    "build": "webpack --mode production",
    "build:validate": "pnpm validate && pnpm build",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "validate": "pnpm lint && pnpm ts-check",
    "format": "prettier --write .",
    "postinstall": "husky"
  }
