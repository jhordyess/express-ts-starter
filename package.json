{
  "name": "express-ts-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "scripts": {
    "start": "node dist/index.cjs",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "build:validate": "pnpm validate && pnpm build",
    "ts-check": "tsc --noEmit",
    "lint": "eslint .",
    "validate": "pnpm lint && pnpm ts-check",
    "format": "prettier --write .",
    "prepare": "husky",
    "postinstall": "pnpm audit --audit-level moderate"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.9",
    "@types/webpack-node-externals": "^3.0.4",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "^3.8.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
